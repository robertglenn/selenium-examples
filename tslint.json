{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "indent": [true, "spaces", 2],
      "max-line-length": [false],
      "ordered-imports": false,
      "quotemark": [true, "single", "avoid-escape"],
      "semicolon": [true, "always"],
      "space-before-function-paren": [true, "always"]
    },
    "rulesDirectory": []
}